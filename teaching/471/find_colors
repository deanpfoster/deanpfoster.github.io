# birds: good
category_names='("bluejay"|"parrot"|"blackbird"|"eagle"|"hummingbird"|"pigeon"|"wren"|"pelican"|"parakeet"|"finch")'
# animals: interesting, but why is cow near mouse?
category_names='("dog"|"cat"|"mouse"|"rat"|"horse"|"cow"|"giraffe"|"zerbra"|"elephant"|"hamster"|"tiger"|"monkey")'
# insects plural: works great excipet lice
category_names='("ants"|"aphids"|"lice"|"fleas"|"spiders"|"beetles"|"moths"|"houseflies"|"weevils"|"caterpillars")'
# insects singular
category_names='("ant"|"aphid"|"louse"|"flea"|"spider"|"beetle"|"moth"|"housefly"|"weevil"|"caterpillar")'

# materials dont work
category_names='("steel"|"iron"|"copper"|"brass"|"wood"|"plastic"|"aluminum")'

# letters dont work
category_names='("a"|"b"|"c"|"d"|"e"|"f"|"g")'

# lengths dont work
category_names='("inch"|"foot"|"mile"|"cm"|"meter"|"km")'

# words not in to 10k
category_names='("happy"|"sad"|"depression"|"ecstatic"|"unhappy")'

#  Seperates into initials and real words
category_names='("feb"|"apr"|"jul"|"jun"|"aug"|"sept"|"oct"|"nov"|"february"|"march"|"april"|"june"|"july"|"august"|"september"|"october"|"november"|"december")'

# at least mouse and rat are close
category_names='("dog"|"cat"|"mouse"|"rat"|"elephant"|"horse"|"cow"|"giraffe"|"zerbra"|"elephant"|"hamster")'
