category_names='("happiness"|"joy"|"pleasure"|"sadness"|"misery"|"displeasure"|"unhappiness"|"anger")'
# emotions these work badly
category_names='("anger"|"fear"|"disgust"|"happiness"|"sadness"|"surprise"|"amusement"|"contempt"|"contentment"|"embarrassment"|"excitement"|"guilt"|"relief"|"satisfaction"|"shame")'
category_names='("anger"|"fear"|"disgust"|"happiness"|"sadness"|"surprise")'
# emotions: try removing the ones that can be verbs: fear, disgust, surprise (
category_names='("anger"|"happiness"|"sadness"|"amusement"|"contempt"|"contentment"|"embarrassment"|"excitement"|"guilt"|"relief"|"satisfaction"|"shame")'
# emotions: also note that we "get" "relief" and "satisfaction", but not the others  -- still crappy
category_names='("anger"|"happiness"|"sadness"|"amusement"|"contempt"|"contentment"|"embarrassment"|"excitement"|"guilt"|"shame")'

# zip codes: none of these are found
